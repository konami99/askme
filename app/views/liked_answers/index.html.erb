<h1>Listing liked_answers</h1>

<table>
  <thead>
    <tr>
      <th>Answer</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @liked_answers.each do |liked_answer| %>
      <tr>
        <td><%= liked_answer.answer %></td>
        <td><%= liked_answer.user %></td>
        <td><%= link_to 'Show', liked_answer %></td>
        <td><%= link_to 'Edit', edit_liked_answer_path(liked_answer) %></td>
        <td><%= link_to 'Destroy', liked_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Liked answer', new_liked_answer_path %>
